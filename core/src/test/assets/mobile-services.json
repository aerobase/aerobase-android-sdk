{
    "version": "1.0",
    "clusterName": "example.aerobase.io",
    "namespace": "example",
    "clientId": "example_client_id",
    "services": [
        {
            "id": "keycloak",
            "name": "keycloak",
            "type": "keycloak",
            "url": "https://example.aerobase.io",
            "config": {
                "auth-server-url": "https://example.aerobase.io/auth",
                "realm": "example",
                "resource": "example-client",
                "ssl-required": "external",
                "url": "https://example.aerobase.io/auth"
            }
        },
        {
            "id": "metrics",
            "name": "metrics",
            "type": "metrics",
            "url": "https://example.aerobase.io/",
            "config": {}
        }
    ]
}
